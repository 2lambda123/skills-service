{
  "name": "@skilltree/common-components",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "dist/common-components.umd.min.js",
  "module": "dist/common-components.esm.min.js",
  "description": "SkillTree Common UI Components",
  "author": "SkillTree Team",
  "private": true,
  "scripts": {
    "licenseDepCheck": "license-checker --excludePrivatePackages --production --onlyAllow='MIT;ISC;Apache-2.0' --summary",
    "licenseHeaderCheck": " npx license-check-and-add check -f '../license-add/license-add-config.json'",
    "licenseHeaderAdd": " npx license-check-and-add add -f '../license-add/license-add-config.json'",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "build": "npm run licenseCheck && npm run test && npm run clean && npm run build:umd && npm run build:esm",
    "build:umd": "rollup --config --format umd --file dist/common-components.umd.min.js --name common-components",
    "build:esm": "rollup --config --format esm --file dist/common-components.esm.min.js",
    "clean": "rm -rfv dist",
    "watch": "npm run clean && rollup -c --watch",
    "test": "npx jest --passWithNoTests",
    "test-watch": "npx jest --passWithNoTests --watch",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "lint": "npx eslint --ext .js,.vue src/**"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@vue/test-utils": "1.1.0",
    "@babel/preset-env": "7.11.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.2",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-vue": "7.0.1",
    "jest": "26.5.3",
    "license-check-and-add": "3.0.4",
    "license-checker": "25.0.1",
    "rollup": "2.29.0",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-peer-deps-external": "2.2.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "vue": "2.6.12",
    "vue-eslint-parser": "7.1.1",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.12",
    "@vue/babel-preset-app": "4.5.13",
    "@fortawesome/fontawesome-free": "5.14.0",
    "bootstrap": "4.5.2",
    "bootstrap-vue": "2.21.2",
    "dayjs": "1.9.6",
    "dompurify": "2.2.6",
    "font-awesome": "4.7.0",
    "marked": "2.0.1",
    "node-emoji": "1.10.0",
    "number-format.js": "2.0.9",
    "vue-js-toggle-button": "1.3.3",
    "vue-radial-progress": "0.3.2",
    "vue-simple-progress": "1.1.1",
    "vue-simple-spinner": "1.2.10"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "bootstrap": "4.5.2",
    "bootstrap-vue": "2.21.2",
    "dayjs": "1.9.6",
    "dompurify": "2.2.6",
    "font-awesome": "4.7.0",
